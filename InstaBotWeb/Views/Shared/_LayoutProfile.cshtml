@{
	Layout = null;
}
<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>UserAccaunt</title>
	@await Html.PartialAsync("ProfileCssPartial")
</head>
<body>
	<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
